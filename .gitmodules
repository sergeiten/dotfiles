[submodule "dotbot"]
	path = dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule ".zsh/plugins/pure"]
	path = .zsh/plugins/pure
	url = git@github.com:sindresorhus/pure.git
[submodule ".zsh/plugins/autosuggestions"]
	path = .zsh/plugins/autosuggestions
	url = git@github.com:zsh-users/zsh-autosuggestions.git
[submodule ".zsh/plugins/syntax-highlighting"]
	path = .zsh/plugins/syntax-highlighting
	url = git@github.com:zsh-users/zsh-syntax-highlighting.git
[submodule ".zsh/plugins/system-clipboard"]
	path = .zsh/plugins/system-clipboard
	url = git@github.com:kutsan/zsh-system-clipboard.git
[submodule ".zsh/plugins/autopair"]
	path = .zsh/plugins/autopair
	url = git@github.com:hlissner/zsh-autopair.git
